<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".oneFiles.addOne"
    >


    <EditText
        android:id="@+id/titleInput"
        android:layout_width="255dp"
        android:layout_height="53dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="79dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="42dp"
        tools:layout_conversion_absoluteWidth="322dp" />

    <ImageButton
        android:id="@+id/saveButton"
        android:layout_width="69dp"
        android:layout_height="65dp"
        android:layout_marginTop="73dp"
        android:src="@android:drawable/ic_menu_save"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.653"
        app:layout_constraintStart_toEndOf="@+id/titleInput"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="51dp"
        tools:layout_conversion_absoluteWidth="56dp" />

    <EditText
        android:id="@+id/amountOne"
        android:layout_width="338dp"
        android:layout_height="106dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="56dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="How many?"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleInput" />

    <Spinner
        android:id="@+id/type_one"
        android:layout_width="299dp"
        android:layout_height="73dp"
        android:layout_marginTop="83dp"
        android:layout_marginEnd="75dp"
        android:entries="@array/type_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/amountOne" />


</androidx.constraintlayout.widget.ConstraintLayout>